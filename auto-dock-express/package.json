    "jest": "^29.2.2",
    "jsonfile": "^6.1.0",
    "rxjs": "^7.5.2",
    "ts-jest": "^29.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.13",
    "del-cli": "^4.0.1",
